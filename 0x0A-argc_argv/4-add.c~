#include<stdio.h>
#include<stdlib.h>
#include<ctype.h>
/**
* main - prints its name, followed by a new line.
* @argc: number of command line arguments.
* @argv: array that contains the program command line arguments.
* Return: 0 - success.
*/
int main(int argc, char *argv[])
{
int r = 0;
int i;
for (i = 1 ; i < argc ; i++)
{
if (!isdigit(argv[i]))
{
printf("Error\n");
return (1);
}
r += atoi(argv[i]);
}
printf("%d\n", r);
return (0);
}
